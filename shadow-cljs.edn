;; shadow-cljs configuration
{  
  :source-paths ["src"]

  :dependencies []

  :builds
  { :main
    { :target :node-script
      :output-to "main.js"
      :main app.main/init

      :compiler-options {}}
  
    :test
    { :target :browser
      :output-dir "resources/js"
      :asset-path "/js"
    
      :compiler-options {}

      :modules 
      {:app {:entries [app.test]}}      
      
      :devtools 
      { :before-load app.test/stop
        :after-load app.test/start}}}}
